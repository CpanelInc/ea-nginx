{
   "add_to_build_requires" : [
      "libaio-dev",
      [
         "libaio1t64",
         "libaio1"
      ],
      "linux-libc-dev",
      "libc6-dev",
      "libcurl4",
      "libcurl4-openssl-dev"
   ],
   "add_to_requires" : {
      "ea-nginx" : [
         [
            "libaio1t64",
            "libaio1",
            "libcurl4"
         ]
      ]
   },
   "adjust_install_file_dst" : [
      [
         "etc/dnf/universal-hooks/multi_pkgs/transaction",
         "etc/apt/universal-hooks/multi_pkgs/Post-Invoke"
      ]
   ],
   "adjust_install_file_src" : [
      [
         "etc/dnf/universal-hooks/multi_pkgs/transaction",
         "etc/apt/universal-hooks/multi_pkgs/Post-Invoke"
      ]
   ],
   "fix_dh_usrlocal" : 1,
   "override_vars" : {
      "BASE_WITH_CC_OPT" : "",
      "BASE_WITH_LD_OPT" : "",
      "_builddir" : "",
      "_datadir" : "/usr/share",
      "_libdir" : "/usr/lib64",
      "_libexecdir" : "/usr/libexec",
      "_localstatedir" : "/var",
      "_sbindir" : "/usr/sbin",
      "_sysconfdir" : "/etc",
      "nginx_loggroup" : "adm",
      "use_systemd" : 1
   },
   "put_source0_in_sources" : true,
   "remove_from_requires" : [
      "libcurl4-gnutls-dev",
      "libopenssl-devel",
      "redhat-lsb-core",
      "initscripts"
   ],
   "remove_from_specific_install" : {
      "ea-nginx" : [
         "/etc/nginx/conf.d/modules/ngx_http_pipelog_module.conf"
      ]
   },
   "source_tarball_regex" : "release"
}
